{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAFroHBrDo8zJfboSR3r5Z_YWnLv9HEaxw&libraries=geometry,visualization"></script>
{% endblock %}
{% block body %}
    <div id="birds"></div>
{% endblock %}
{% block javascripts %}
    <script>
        {# set a base url to '/' or maybe '/app_dev.php' #}
        window.baseUrl = '{{ path('bird') }}';
    </script>
    {% if app.environment == 'dev' %}
        <script src="http://127.0.0.1:3000/static/searchBird.js"></script>
    {% else %}
        <script src="{{ asset('dist/searchBird.js') }}"></script>
    {% endif %}
{% endblock %}